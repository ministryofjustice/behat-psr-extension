{
  "name": "cooperaj/behat-psr-extension",
  "description": "Provides a bootstrapped PSR7 application and PSR11 container to your behat contexts",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Adam Cooper",
      "email": "adam@acpr.dev"
    }
  ],
  "require": {
    "php": "^8.3",
    "friends-of-behat/mink-browserkit-driver": "^1.6.1",
    "friends-of-behat/mink-extension": "^2.4",
    "symfony/http-foundation": "^5.1||^6.4",
    "symfony/psr-http-message-bridge": "^2.0||^6.4",
    "symfony/http-kernel": "^5.1||^6.4"
  },
  "autoload": {
    "psr-4": {
      "Acpr\\Behat\\Psr\\": "src/",
      "Acpr\\Behat\\Psr\\FeatureContexts\\": "tests/features/contexts/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TestAcpr\\Behat\\Psr\\": "tests/unit"
    }
  },
  "require-dev": {
    "laminas/laminas-diactoros": "^2.2",
    "laminas/laminas-config-aggregator": "^1.1",
    "laminas/laminas-servicemanager": "^3.4",
    "mezzio/mezzio": "^3.2",
    "mezzio/mezzio-fastroute": "^3.0",
    "behat/behat": "^3.5",
    "phpunit/phpunit": "^12.0",
    "phpspec/prophecy-phpunit": "^2.0",
    "vimeo/psalm": "^6.0",
    "infection/infection": ">=0.15",
    "psalm/plugin-phpunit": "^0.19.5",
    "psalm/plugin-symfony": "^5.3"
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
